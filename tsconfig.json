{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/src/*": [
        "./src/*"
      ],
      "@/domain/*": [
        "./src/domain/*"
      ],
      "@/infrastructure/*": [
        "./src/infrastructure/*"
      ],
      "@/presentation/*": [
        "./src/presentation/*"
      ],
      "@/shared/*": [
        "./src/shared/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
